{
  "name": "backend_todolist",
  "version": "1.0.0",
  "description": "Aplicação backend em NodeJs integrada com MongoDB",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/**/*$NAME*.test.js --exit",
    "prepare": "husky install",
    "gcmsg": "lint-staged",
    "dev": "nodemon index.js",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json ."
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cristianocsl/backend_todolist.git"
  },
  "keywords": [
    "ToDo-list",
    "Nodejs",
    "MongoDB",
    "React"
  ],
  "author": "Cristiano Seabra de Lima",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cristianocsl/backend_todolist/issues"
  },
  "homepage": "https://github.com/cristianocsl/backend_todolist#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "chai": "^4.3.6",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.9.0",
    "eslint-config-trybe-frontend": "^1.2.3",
    "eslint-plugin-sonarjs": "^0.11.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "mocha": "^9.2.0",
    "mongodb-memory-server": "^6.9.6",
    "sinon": "^13.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "eslint-config-trybe-backend": "^1.0.4",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.3.0",
    "mongodb": "^4.3.1",
    "nodemon": "^2.0.15"
  }
}
